{
  "name": "@wilson/remark-vscode",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Christoph Werner <christoph@codepunkt.de>",
  "description": "Blazing-fast, opinionated static site generator for Preact",
  "bin": {
    "wilson": "bin/create-wilson-site.js"
  },
  "type": "module",
  "exports": "./index.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "node scripts/index.js",
    "prebuild": "git submodule update --init --depth 1"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module"
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    }
  },
  "dependencies": {
    "cache-manager": "^3.4.4",
    "decompress": "^4.2.1",
    "graphql": "^15.5.1",
    "loglevel": "^1.7.1",
    "typescript": "^4.3.4",
    "unist-util-visit": "^3.1.0",
    "vscode-oniguruma": "^1.5.1",
    "vscode-textmate": "^5.4.0"
  }
}
